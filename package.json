{
  "name": "my-version",
  "version": "0.0.1",
  "description": "",
  "main": "server.js",
  "scripts": {
    "start": "node build/server/server.js",
    "build": "npm-run-all -s build-ts build-browserify",
    "watch": "npm-run-all -r -p watch-*",
    "watch-node": "nodemon build/server/server.js",
    "build-ts": "tsc",
    "watch-ts": "tsc -w",
    "build-browserify": "browserify  ./build/client/main.js -t [ envify purge --STUN_URL $STUN_URL --TRACKER_URL $TRACKER_URL ] -o ./public/js/bundle.js",
    "watch-browserify": "watchify  ./build/client/main.js -t [ envify purge --STUN_URL $STUN_URL --TRACKER_URL $TRACKER_URL ] -o ./public/js/bundle.js",
    "tslint": "tslint -c tslint.json -p tsconfig.json",
    "export": "export STUN_IP=\"vm2-internal.devclub.in\"; export TRACKER_IP=\"vm2-internal.devclub.in\""
  },
  "author": "",
  "license": "MIT",
  "dependencies": {
    "body-parser": "^1.19.0",
    "debug": "^4.1.1",
    "express": "^4.17.1",
    "grunt": "^1.0.4",
    "node": "^12.4.0",
    "socket.io": "^2.2.0",
    "thunky": "^1.0.3",
    "webtorrent": "^0.103.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/debug": "^4.1.4",
    "@types/events": "^3.0.0",
    "@types/express": "^4.17.0",
    "@types/jquery": "^3.3.29",
    "@types/node": "^12.0.4",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "@types/webtorrent": "^0.98.5",
    "browserify": "^16.2.3",
    "envify": "^4.1.0",
    "eslint": "^5.16.0",
    "eslint-config-airbnb": "^17.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "nodemon": "^1.19.1",
    "npm-run-all": "^4.1.5",
    "tslint": "^5.17.0",
    "typescript": "^3.5.1",
    "watchify": "^3.11.0"
  }
}
