{
  "name": "sendata",
  "version": "0.0.1",
  "description": "Simple tool for sending file in an Intranet environment",
  "main": "src/server/server.js",
  "scripts": {
    "start": "node src/server/server.js",
    "tsc": "tsc -p ./lib/server && tsc -p ./lib/client",
    "build": "npm run tsc && browserify  ./src/client/main.js -t [ envify --STUN_IP $STUN_IP --TRACKER_IP $TRACKER_IP ] > ./js/bundle.js",
    "watch": "watchify ./src/client/main.js -o ./js/bundle.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/devclub-iitd/sendata.git"
  },
  "license": "MIT",
  "dependencies": {
    "browserify": "^16.2.2",
    "envify": "^4.1.0",
    "express": "^4.16.4",
    "grunt": "^1.0.1",
    "socket.io": "^2.2.0",
    "thunky": "^1.0.2",
    "typescript": "^3.4.5",
    "webrtc-adapter": "^6.0.3"
  },
  "engines": {
    "node": ">=6.11.1"
  },
  "devDependencies": {
    "@types/body-parser": "^1.17.0",
    "@types/express": "^4.16.1",
    "@types/jquery": "^3.3.29",
    "@types/node": "^12.0.2",
    "@types/socket.io": "^2.1.2",
    "@types/socket.io-client": "^1.4.32",
    "tslint": "^5.8.0",
    "typescript": "^2.5.3",
    "eslint": "^4.19.1",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-plugin-import": "^2.12.0",
    "eslint-plugin-jsx-a11y": "^6.0.3",
    "eslint-plugin-react": "^7.8.2",
    "watchify": "^3.11.0"
  }
}
